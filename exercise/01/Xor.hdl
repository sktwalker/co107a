/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Or(a=in[0], b=in[1], out=a);
	Or(a=in[2], b=in[3], out=b);
	Or(a=in[4], b=in[5], out=c);
	Or(a=in[6], b=in[7], out=d);
	Or(a=a, b=b, out=e);
	Or(a=c, b=d, out=f);
	Or(a=e, b=f, out=out);
}
